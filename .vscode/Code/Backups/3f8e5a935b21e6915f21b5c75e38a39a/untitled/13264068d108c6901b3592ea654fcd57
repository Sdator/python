firewall-cmd --add-rich-rule='rule family="ipv4" source address="14.209.0.0/16" service name="ssh" accept'

firewall-cmd --zone=internal --add-rich-rule='rule family="ipv4" source address="14.209.0.0/16" service name="ssh" accept'

firewall-cmd --permanent --add-source="103.27.1.1/16" --add-source="112.96.1.1/16"
firewall-cmd --permanent --zone=internal --add-source="103.27.0.0/16" --add-source="112.96.0.0/16"

firewall-cmd --permanent --remove-source="103.27.1.1/16" --remove-source="112.96.1.1/16"

# 添加地址  source
firewall-cmd --permanent --add-source="103.27.0.0/16" --add-source="112.96.0.0/16"
firewall-cmd --permanent --add-source="103.27.0.0/16" 
firewall-cmd --permanent --add-source="116.29.1.1/16"
firewall-cmd --permanent --zone=internal --add-source="116.29.1.1/16"



# 添加端口  port
firewall-cmd --permanent --add-port="12992/tcp" --add-port="12992/udp"
firewall-cmd --permanent --zone=internal --add-port="12993/tcp" --add-port="12993/udp"
firewall-cmd --permanent --zone=public --add-port="12993/tcp" --add-port="12993/udp"

#移除端口
firewall-cmd --permanent --remove-port="12992/tcp" --remove-port="12992/udp"


# 给指定地址添加例外端口
firewall-cmd --permanent --add-rich-rule='rule family="ipv4" source address="103.27.0.0/16" port port="12992" protocol="tcp" accept'
firewall-cmd --permanent --add-rich-rule='rule family="ipv4" source address="112.96.0.0/16" port port="12992" protocol="tcp" accept'
firewall-cmd --permanent --add-rich-rule='rule family="ipv4" source address="116.29.0.0/16" port port="12992" protocol="tcp" accept'


firewall-cmd --add-rich-rule='rule family="ipv4" source address="103.27.0.0/16" accept'
firewall-cmd --add-rich-rule='rule family="ipv4" source address="112.96.0.0/16" accept'






firewall-cmd --reload

